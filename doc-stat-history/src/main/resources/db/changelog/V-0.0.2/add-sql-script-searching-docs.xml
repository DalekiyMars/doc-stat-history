<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                            http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="add-sql-script-search-docs" author="Eugene">
        <sqlFile path="db.procedures/search-documents-with-cursor.sql"
                 relativeToChangelogFile="false"
                 splitStatements="false"
                 stripComments="false"/>

        <rollback>
            DROP FUNCTION IF EXISTS entity_tables.search_documents_with_cursor(
            VARCHAR, VARCHAR, TIMESTAMP, TIMESTAMP, BIGINT, INT
            );
        </rollback>
    </changeSet>
</databaseChangeLog>